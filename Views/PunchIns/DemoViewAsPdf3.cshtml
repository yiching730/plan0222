@model IEnumerable<plan02.Models.PunchIn>

@{
    ViewData["Title"] = "DemoViewAsPdf3";
}

<h1>出勤簽到資料</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                年
            </th>
            <th>
                月
            </th>
            <th>
                日期
            </th>
            <th>
                上班時間
            </th>
            <th>
                下班時間
            </th>
            <th>
                請假備註
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @item.Year
            </td>
            <td>
               @item.Month
            </td>
            <td>
               @item.Day
            </td>
            <td>
                @item.PunchIn1
            </td>
            <td>
                @item.PunchOut
            </td>
            <td>
                @item.Remark2
            </td>
        </tr>
        }
    </tbody>
</table>


