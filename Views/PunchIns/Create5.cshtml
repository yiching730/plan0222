@model plan02.Models.PunchIn

    @{
        ViewData["Title"] = "Create5";
    }

    @{
    int year = DateTime.Now.Year;
    int month = DateTime.Now.Month;
    int daysInMonth = DateTime.DaysInMonth(year, month);
}


<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script>
$(document).ready(function () {   
    $('#selectlist').change(function () {
        var value = $(this).val();
        if (value == '2') {
              $("#day31").hide();
              $("#day30").hide();
              $("#day29").hide();
              $(".Allmonth").val($(this).val());
        } else if ((value == '4') || (value == '6') || (value == '9') || (value == '11')) {
                $("#day30").show();
                $("#day29").show();
                $("#day31").hide();
                $(".Allmonth").val($(this).val());
        } 
        else if ((value == '1') || (value == '3') || (value == '5') || (value == '7')|| (value == '8') || (value == '10') || (value == '12')) {
		         $("#day31").show();
                 $(".Allmonth").val($(this).val());
            }
    });
});

</script>

<script>
    $(document).ready(function () {   
    $('#selectlist_year').change(function () {
        var value = $(this).val();
        if (value != null) {
              $(".Allyear").val($(this).val());
        }
    });
});
</script>


