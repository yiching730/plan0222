@model plan02.Models.AEmployee

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>AEmployee</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="AId" />
            <div class="form-group">
                <label asp-for="UserName" class="control-label"></label>
                <input asp-for="UserName" class="form-control" />
                <span asp-validation-for="UserName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AName" class="control-label"></label>
                <input asp-for="AName" class="form-control" />
                <span asp-validation-for="AName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AGender" class="control-label"></label>
                <input asp-for="AGender" name="agender" class="form-control" />
                @*<select asp-for="AGender" class="form-control">
                    <option value="專任助理">專任助理</option>
                    <option value="獎助生(學習型)兼任助理">獎助生(學習型)兼任助理</option>
                    <option value="勞僱型兼任助理">勞僱型兼任助理</option>
                    <option value="勞僱型臨時工(臨時僱用且無專職工作)">勞僱型臨時工(臨時僱用且無專職工作)</option>
                </select>*@
                <span asp-validation-for="AGender" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AMail" class="control-label"></label>
                <input asp-for="AMail" class="form-control" />
                <span asp-validation-for="AMail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ASalary" class="control-label"></label>
                <input asp-for="ASalary" class="form-control" />
                <span asp-validation-for="ASalary" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AEmploymentDate" class="control-label"></label>
                <input asp-for="AEmploymentDate" class="form-control" />
                <span asp-validation-for="AEmploymentDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}



